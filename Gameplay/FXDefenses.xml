			<KeyValueInt>
				<Key>DiffuseGradientRow</Key>
				<Value>13</Value>
			</KeyValueInt>
      <KeyValueDictionaryRef>TargetObjectTargetMobileObjectOrigin</KeyValueDictionaryRef>
    </FXElement>
  </FXElement>

