        </KeyValueString>

        <KeyValueDictionaryRef>SourceObjectSourceMobileObject</KeyValueDictionaryRef>
        <KeyValueDictionaryRef>TargetObjectTargetMobileObject</KeyValueDictionaryRef>
      </FXElement>

    </FXElement>
    
  </FXElement>

  <FXElement>
    <InternalName>BattleViewerDefaultAnimText</InternalName>
    <Type>AnimatedText</Type>

    <KeyValueString>
      <Key>FontDefName</Key>
