    <KeyValueBool>
      <Key>OffsetAboveBounds</Key>
      <Value>true</Value>
    </KeyValueBool>
    <KeyValueVec3>
      <Key>Offset</Key>
      <Value>
        <X>0</X>
        <Y>20</Y>
        <Z>0</Z>
