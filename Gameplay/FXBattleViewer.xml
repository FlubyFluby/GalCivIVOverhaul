  <FXElement>
    <InternalName>BattleViewerBattleFireShot</InternalName>
    <Type>Composite</Type>
    
    <FXElement>
      <InternalName>BattleViewerBattleFireShot_VolleySequence</InternalName>
      <Type>Sequence</Type>

      <FXElement>
        <InternalName>BattleViewerBattleFireShot_Volley</InternalName>
        <Type>Volley</Type>
        <KeyValueString>
          <Key>DamageType</Key>
