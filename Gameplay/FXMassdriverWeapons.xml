				<KeyValueDecimal>
					<Key>BattleViewerUniformScale</Key>
					<Value>1.0</Value>
				</KeyValueDecimal>
				<KeyValueBool>
					<Key>IgnoreParentScale</Key>
					<Value>true</Value>
				</KeyValueBool>
				<KeyValueBool>
					<Key>PointParticleYDownParentZ</Key>
					<Value>true</Value>
