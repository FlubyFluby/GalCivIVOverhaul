	<!-- Kinetic Weapon Sequence - Kinetic Cannon -->
	<FXElement>
		<InternalName>KineticCannon</InternalName>
		<Type>Composite</Type>

		<FXElement>
			<InternalName>KineticCannon_Projectile</InternalName>
			<Type>Projectile</Type>
			<KeyValueDecimal>
				<Key>BattleViewerProjectileSpeed</Key>
				<Value>700.0</Value>
			</KeyValueDecimal>
			<KeyValueDecimal>
				<Key>MainMapProjectileSpeed</Key>
				<Value>175.0</Value>
			</KeyValueDecimal>

			<FXElement>
				<InternalName>KineticCannon_Particle</InternalName>
				<Type>Particle</Type>
				<KeyValueString>
					<Key>Script</Key>
					<Value>KineticCannonParticleEffect</Value>
				</KeyValueString>
				<KeyValueDecimal>
